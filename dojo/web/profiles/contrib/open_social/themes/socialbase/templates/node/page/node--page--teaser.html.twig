{% extends "node--teaser.html.twig" %}

{% block card_image %}
  {{ content.field_page_image }}
{% endblock %}

{% block card_teaser_type %}
  {% embed "node--teaser__type.html.twig" %}
    {%- block card_teaser_type -%} page {%- endblock -%}
  {% endembed %}
{% endblock %}

{% block card_body %}

{% embed "node--teaser__body.html.twig" %}
    {% block field_body %} {{ content.body }} {% endblock %}
{% endembed %}

{% endblock %}
